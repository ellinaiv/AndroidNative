<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_map">

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.example.team11.ui.map.MapFragment"
        android:label="@string/title_map"
        tools:layout="@layout/fragment_map" />

    <fragment
        android:id="@+id/navigation_list"
        android:name="com.example.team11.ui.placesList.PlacesListFragment"
        android:label="@string/title_list"
        tools:layout="@layout/fragment_places_list" />

    <fragment
        android:id="@+id/navigation_favorites"
        android:name="com.example.team11.ui.favorites.HomeFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_favorites" />
</navigation>
